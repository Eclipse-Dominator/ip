            <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
         </padding>
         <HBox.margin>
            <Insets left="7.0" right="7.0" />
         </HBox.margin>
      </HBox>
    <ImageView fx:id="displayPicture" fitHeight="99.0" fitWidth="99.0" pickOnBounds="true" preserveRatio="true" style="-fx-effect: dropshadow(three-pass-box, rgba(0, 0, 0, 0.8), 10, 0, 0, 0);;" />
  </children>
  <padding>
    <Insets bottom="15.0" left="5.0" right="5.0" top="15.0" />
  </padding>
</fx:root>
